                    return <p>{stat.stat.name} - {stat.base_stat}</p>\n                        })}\n                    </div>\n                    <div className={'item'}>\n                        <h1>Abilities</h1>\n                        {pokeData.abilities.map(ability => {\n                            return <p>{ability.ability.name}</p>\n                        })}\n                    </div>\n                </div>\n            )\n        }\n        if (pokemonState.loading) {\n            return <p>loading...</p>\n        }\n        if (pokemonState.errorMsg !== '') {\n            return <p>{pokemonState.errorMsg}</p>\n        }\n        return <p>unable to get data</p>\n    }\n    return (\n        <div className={'pokemon'}>\n            <h1>{pokemonName}</h1>\n            {ShowData()}\n        </div>\n    )\n};\n\nexport default Pokemon","/home/igo/Pulpit/redux_pokemon/src/redux/reducers/rootReducer.js",[],"/home/igo/Pulpit/redux_pokemon/src/redux/reducers/pokemonListReducer.js",[],"/home/igo/Pulpit/redux_pokemon/src/redux/actions.js",[],"/home/igo/Pulpit/redux_pokemon/src/redux/reducers/pokemonMultipleReducer.js",[],{"ruleId":"44","replacedBy":"45"},{"ruleId":"46","replacedBy":"47"},{"ruleId":"48","severity":1,"message":"49","line":14,"column":8,"nodeType":"50","endLine":14,"endColumn":10,"suggestions":"51"},{"ruleId":"48","severity":1,"message":"52","line":13,"column":8,"nodeType":"50","endLine":13,"endColumn":10,"suggestions":"53"},"no-native-reassign",["54"],"no-negated-in-lhs",["55"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'FetchData'. Either include it or remove the dependency array.","ArrayExpression",["56"],"React Hook useEffect has missing dependencies: 'dispatch' and 'pokemonName'. Either include them or remove the dependency array.",["57"],"no-global-assign","no-unsafe-negation",{"desc":"58","fix":"59"},{"desc":"60","fix":"61"},"Update the dependencies array to be: [FetchData]",{"range":"62","text":"63"},"U